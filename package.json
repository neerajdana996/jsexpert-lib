{
  "name": "jsexpert-lib",
  "version": "0.21.1",
  "type": "module",
  "module": "./dist/jsexpert-lib.js",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/jsexpert-lib.js",
      "require": "./dist/jsexpert-lib.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "test": "echo Hello",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "build": "tsc",
    "prepare": "npm run build",
    "preview": "vite preview",
    "push": "yarn build && git add . && git commit -m 'new build' && git push --set-upstream origin master && npm version minor   && npm publish "
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.1",
    "prettier": "^2.8.8",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^2.3.0"
  },
  "files": [
    "dist"
  ],
  "description": "This is a simple utility package",
  "author": "Neeraj Dana",
  "license": "MIT",
  "homepage": "Jsexpert.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/neerajdana996/jsexpert-lib"
  },
  "bugs": {
    "url": "https://github.com/neerajdana996/jsexpert-lib/issues"
  },
  "keywords": [
    "jsexpert"
  ]
}
